<h1>Contact page</h1>
<h2>Meet my team</h2>
<form action="/contact" method="get">
  <input type="text"
         name="member"
         value="<%= params[:member] %> "
         placeholder="who are you looking for ?">
  <input type="submit">
</form>
<ol>
  <% @members.each do |member| %>
  <li><%= member %></li>
  <% end %>
</ol>

<%= link_to "retour home page", root_path %>

